{
  "name": "zsgrammar",
  "displayName": "ZSGrammar",
  "description": "Grammar for zero sievert modding",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "catspeak",
      "aliases": ["catspeak"],
      "extensions": [".script", ".meow"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "catspeak",
      "scopeName": "source.catspeak",
      "path": "./syntaxes/catspeak.tmLanguage.json"
    }],
    "snippets": [{
        "language": "catspeak",
        "path": "./snippets/snippets.json"
    }]
  }
}
